<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="assets_editor" inherit_id="website.assets_editor" name="Tag Cloud Editor">
    <xpath expr="." position="inside">
        <script type="text/javascript" src="/website_tags_cloud/static/src/snippets/s_tags_cloud/options.js"></script>
    </xpath>
</template>


<template id="assets_frontend" inherit_id="website.assets_frontend" name="Tag Cloud Front-end assets">
    <xpath expr="." position="inside">
         <link rel="stylesheet" type="text/scss" href="/website_tags_cloud/static/src/snippets/s_tags_cloud/000.scss"/>

        <script type="text/javascript" src="/website_tags_cloud/static/src/snippets/s_tags_cloud/000.js"/>
    </xpath>
</template>
 


</odoo>