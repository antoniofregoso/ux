<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_logo_cover" name="Logo Cover">
    <section class="s_cover parallax s_parallax_is_fixed bg-black-50 pt96 pb96" data-scroll-background-ratio="1">
        <span class="s_parallax_bg oe_img_bg" style="background-image: url('/web/image/website_caramba_snippets.s_screen_cover'); background-position: 50% 0;"/>
        <div class="o_we_bg_filter bg-black-50"/>
        <div class="container s_allow_columns">
            <div class="row">
                <div class="col-12 align-top">
                    <img alt="Company team" src="/web/image/website_caramba_snippets.s_screen_logo" class="img-fluid d-block mx-auto" style="width: 300px;"/>
                </div>
                <div class="o_colored_level col-lg-12">
                    <h1 style="text-align: center;">
                    <font style="font-size: 62px; font-weight: bold;">Catchy Headline</font>
                    </h1>
                    <p class="lead" style="text-align: center;">
                    Write one or two paragraphs describing your product, services or a specific feature.
                    <br/>
                    To be successful your content needs to be useful to your readers.
                    </p>
                    <p style="text-align: center;">
                    <a href="/contactus" class="btn btn-primary">Contact us</a>
                    </p>
                </div>
            </div>
                <div class="row crmb-social">
                    <div class="col-sm-12 text-right js_get_social" data-template="website_caramba_snippets.s_social_template"/>
                </div>
        </div>
    </section>
</template>

</odoo>