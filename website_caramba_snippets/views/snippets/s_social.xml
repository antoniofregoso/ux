<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="s_social_template">
    <t t-foreach="social_media" t-as="s">
    <a t-att-href="s['url']" class="btn btn-sm btn-link">
    <i t-att-class="'fa fa-2x ' + s['icon']"/>
    </a>
    </t>
</template>

<!-- Assets -->
<template id="assets_snippet_s_latest_posts_js_000" inherit_id="website.assets_frontend">
    <xpath expr="//script[last()]" position="after">
        <script type="text/javascript" src="/website_caramba_snippets/static/src/snippets/s_social/000.js"/>
    </xpath>
</template>

</odoo>