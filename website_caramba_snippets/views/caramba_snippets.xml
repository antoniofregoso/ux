<template id="s_social_template">
   <t t-foreach="social_media" t-as="s">
    	<a t-att-href="s['url']" class="btn btn-sm btn-link"><i t-att-class="'fa fa-2x ' + s['icon']"/></a> 
    </t>
 </template>

