<?xml version="1.0" encoding="utf-8"?>
<odoo>

<template id="assets_frontend" inherit_id="portal.assets_frontend" name="blog Floating Menu  Front-end assets">
    <xpath expr="//link[last()]" position="after">
        <link rel="stylesheet" type="text/scss" href="/website_animations/static/src/css/animate.min.css"/>
	 </xpath>
	<!-- <xpath expr="//script[last()]" position="after">
        <script type="text/javascript" src="/website_animations/static/src/js/animate.js"/>
    </xpath> --> 
</template>

<template id="assets_editor" inherit_id="website.assets_editor" name="Blog Editor">
    <xpath expr="." position="inside">
        <script type="text/javascript" src="/website_animations/static/src/js/snippets.options.js"></script>
    </xpath>
</template>

</odoo>