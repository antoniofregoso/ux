<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<record id="res_config_settings_view_form" model="ir.ui.view">
            <field name="name">res.config.settings.view.form.inherit.google_maps</field>
            <field name="model">res.config.settings</field>
            <field name="inherit_id" ref="website.res_config_settings_view_form"/>
            <field name="arch" type="xml">
            	 <xpath expr="//div[@id='google_maps_setting']/div[hasclass('o_setting_right_pane')]/div[hasclass('content-group')]" position="inside">
            	 	<div class="row">
                   		<label for="google_map_theme_id" string="Maps Theme" class="col-md-3 o_light_label"/>
                        <field name="google_map_theme_id"/>
                     </div>
            	 </xpath>
            </field>
    </record>

</odoo>