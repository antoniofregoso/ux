<odoo>

	    <template id="company_google_map" inherit_id="website_google_map.google_map" >
	    <xpath expr="//body" position="replace">	
	    	<body t-att-data-partner-url="partner_url or None" style="height: 100%; margin: 0; padding: 0;">
	    		 <div id="map" style="height: 100%;"></div>
	    		 <script>
	    		 	function initMap() {
	    		 	var map = new google.maps.Map(document.getElementById('map'), {
	    		 	center: {lat: <t t-raw="lat"/>, lng: <t t-raw="lng"/>},
	    		 	zoom: 12,
	    		 	styles: <t t-raw="styles"/>
				        });
				      }
	    		 </script>
	    		 <t t-if="google_maps_api_key">
		            <script t-attf-src="https://maps.google.com/maps/api/js?key=#{google_maps_api_key}"></script>
		        </t>
		        <t t-else="1">
		            <script src="https://maps.google.com/maps/api/js"></script>
		        </t>
		    </body>
	    </xpath>
	    </template>

</odoo>